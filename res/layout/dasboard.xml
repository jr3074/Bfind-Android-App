<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    
<TableLayout
    android:id="@+id/widget58"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff"
    android:orientation="vertical" >

<TableRow
	android:id="@+id/widget72"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">
<TextView
	android:id="@+id/widget78"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Username"
	android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtUsername"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:editable="false"
    android:ems="10"
    android:hint="Username" />

</TableRow>
<TableRow
	android:id="@+id/widget81"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">
<TextView
	android:id="@+id/widget83"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Password"
	android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtPassword"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="Password"
    android:inputType="textPassword" >

    <requestFocus />
</EditText>

</TableRow>
<TableRow
	android:id="@+id/widget85"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">
<TextView
	android:id="@+id/widget86"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text="Nama"
	android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtNama"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:hint="Nama"
    android:inputType="textPersonName"
    android:textSize="18sp" />

</TableRow>
<TableRow
	android:id="@+id/widget88"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

<TextView
    android:id="@+id/widget89"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Telepon"
    android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtTelp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="08xxxxxxxxxx"
    android:inputType="phone" />

</TableRow>
<TableRow
	android:id="@+id/widget94"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

<TextView
    android:id="@+id/widget95"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Alamat"
    android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtAlamat"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="10"
    android:hint="Alamat"
    android:inputType="textPostalAddress" />

</TableRow>
<TableRow
	android:id="@+id/widget97"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

<TextView
    android:id="@+id/widget98"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Jumlah Kamar"
    android:textColor="#ff000000" />

<EditText
    android:id="@+id/txtJmlKamar"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:hint="Jumlah kamar"
    android:inputType="phone"
    android:textSize="18sp" />

</TableRow>
<TableRow
	android:id="@+id/widget100"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Harga"
        android:textColor="#ff000000" />

    <EditText
        android:id="@+id/txtharga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        android:hint="Harga" />

</TableRow>

<TableRow
	android:id="@+id/widget101"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

    <Button
        android:id="@+id/btCariLongLat"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cari Titik" />
   
    <EditText
        android:id="@+id/txtLat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Lattitude" />

</TableRow>



<TableRow
	android:id="@+id/widget102"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">
 <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff000000" />
  
 <EditText
     android:id="@+id/txtLong"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:ems="10"
     android:hint="Longitude" />

</TableRow>

<TableRow
            android:id="@+id/widget103"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/jenistxt "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Untuk"
                android:textColor="#ff000000" />

            <RadioGroup
                android:id="@+id/jenis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RadioButton
                    android:id="@+id/laki"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Laki-Laki" />

                <RadioButton
                    android:id="@+id/perempuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Perempuan" />
            </RadioGroup>
        </TableRow>
        
        <TableRow
            android:id="@+id/widget104"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/statustxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Status"
                android:textColor="#ff000000" />

            <RadioGroup
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RadioButton
                    android:id="@+id/kosong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="kosong" />

                <RadioButton
                    android:id="@+id/penuh"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="penuh" />
            </RadioGroup>
        </TableRow>

<Button
    android:id="@+id/btUpdate"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/btsimpan" />

<Button
    android:id="@+id/btLogout"
    style="?android:attr/buttonStyleSmall"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Keluar" />

</TableLayout>
</ScrollView>